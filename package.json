  "repository": {
    "type": "git",
    "url": "git+https://github.com/geiserman/package-template.git"
  },
  "homepage": "https://github.com/geiserman/package-template#readme",
  "dependencies": {
    "@geiserman/js-linters": "^1.0.17",
    "@geiserman/js-logger": "^1.0.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/@geiserman/js-linters/linters/eslintrc.js"
  },
  "prettier": "./node_modules/@geiserman/js-linters/linters/prettier.config.js"
}
